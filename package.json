{
  "name": "storybook-dark-mode",
  "version": "4.0.2",
  "description": "Toggle between light and dark mode in Storybook",
  "keywords": [
    "storybook-addons",
    "appearance"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hipstersmoothie/storybook-dark-mode"
  },
  "license": "MIT",
  "author": {
    "name": "Andrew Lisowski",
    "email": "lisowski54@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./cjs/index.d.ts",
        "default": "./cjs/index.js"
      }
    }
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.mjs",
  "types": "./esm/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "src",
    "preset.js",
    "!**/*.stories.tsx"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "unbuild && ncp ./package.cjs.json ./cjs/package.json",
    "prepack": "unbuild && ncp ./package.cjs.json ./cjs/package.json",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext .ts --ext .tsx src/**",
    "release": "auto shipit",
    "sb": "storybook dev",
    "sb:build": "storybook build"
  },
  "dependencies": {
    "@storybook/icons": "^1.4.0",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@storybook/builder-vite": "^9.1.0-alpha.1",
    "@storybook/react-vite": "^9.1.0-alpha.1",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "all-contributors-cli": "^6.24.0",
    "auto": "^11.1.1",
    "auto-config-hipstersmoothie": "^4.0.0",
    "ncp": "^2.0.0",
    "npm-run-all2": "^8.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "storybook": "^9.1.0-alpha.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.5.0",
    "vite": "^5.1.3"
  },
  "packageManager": "yarn@1.22.22",
  "auto": {
    "extends": "hipstersmoothie"
  }
}
